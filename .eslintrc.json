{
  "env": {
      "node": true,
      "jest": true
  },
  "extends": [
      "plugin:@typescript-eslint/recommended",
      "eslint:recommended"
  ],
  "parser": "@typescript-eslint/parser",
  "parserOptions": {
      "ecmaVersion": 12,
      "sourceType": "script"
  },
  "plugins": [
      "@typescript-eslint",
      "prettier"
  ],
  "rules": {
  },
  "overrides": [
    {
      "files": ["webpack.config.js"],
      "rules": {
        "@typescript-eslint/no-var-requires": ["off"]
      }
    }
  ]
}

